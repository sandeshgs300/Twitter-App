{
    "displayName": "Jive SDK - External File Storage",
    "description": "Implements a simple ESP using the Jive SDK and Node.js",
    "registerPlace": "%serviceURL%/fsstorage/register",
    "userMappingOptions": [
        "EMAIL"
    ],
    "supportedResourceNames": [
        "uploadFile",
        "downloadFile"
    ],
    "prefixUrl": "%serviceURL%",
    "resolveResources": "%serviceURL%/resolveResources",
    "icons": {
        "16": "extension-16.png",
        "48": "extension-48.png",
        "128": "extension-128.png"
    },
    "healthCheckURL": "%serviceURL%/fsstorage/isAlive"
}
